	<xs:simpleType name="assetStatusKind">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Unknown"/>
			<xs:enumeration value="Quarantined">
				<xs:annotation>
					<xs:documentation>The asset has been separated from stock and will not be issued. This might for instance occur following receiving, such as with meters, when a subset of each shipment must be tested prior to acceptance of the shipment of meters.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="InInventory">
				<xs:annotation>
					<xs:documentation>The asset has been received and placed into inventory</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CheckedOut">
				<xs:annotation>
					<xs:documentation>The asset has been issued to a worker or crew.  This might occur so that the asset can be installed or might be for maintenance of truck stock.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Installed">
				<xs:annotation>
					<xs:documentation>This asset has been installed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="InService">
				<xs:annotation>
					<xs:documentation>This asset has been installed and has been placed into active service.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OutOfService">
				<xs:annotation>
					<xs:documentation>This asset has been removed from active service, but remains installed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Retired">
				<xs:annotation>
					<xs:documentation>This asset has been retired and will not be reused by its current owner.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Destroyed">
				<xs:annotation>
					<xs:documentation>The asset has been destroyed or otherwise disposed of.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>

							<xs:documentation>The phase(s) that are experiencing outage conditions.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="circuitElementState" type="enum:presumedElementStateType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>

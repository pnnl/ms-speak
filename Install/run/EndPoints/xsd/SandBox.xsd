			<xs:extension base="msp:mspReferable">
				<xs:sequence>
					<xs:element name="changedCircuitElements" type="circuitElements" minOccurs="0"/>
					<xs:element name="createdCircuitElements" type="circuitElements" minOccurs="0"/>
					<xs:element name="deletedCircuitElements" type="msp:objectDeletions" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
